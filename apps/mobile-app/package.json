{
  "name": "@speakbetter/mobile-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --passWithNoTests",
    "lint": "eslint .",
    "pods": "cd ios && pod install && cd ..",
    "build": "echo 'Skipping build for mobile-app'",
    "android:dev": "ENVFILE=.env.development react-native run-android",
    "ios:dev": "ENVFILE=.env.development react-native run-ios",
    "android:prod": "ENVFILE=.env.production react-native run-android",
    "ios:prod": "ENVFILE=.env.production react-native run-ios",
    "env:setup": "npx react-native link react-native-config && npm run pods",
    "start:test": "ENTRY_FILE='index.test.js' react-native start",
    "ios:test": "ENTRY_FILE='index.test.js' ENVFILE=.env.development react-native run-ios",
    "android:test": "ENTRY_FILE='index.test.js' ENVFILE=.env.development react-native run-android",
    "env:dev": "cp .env.development .env && echo 'Switched to dev environment'",
    "env:prod": "cp .env.production .env && echo 'Switched to production environment'",
    "env:check": "node -e \"const config = require('react-native-config'); console.log('Current environment:', config.APP_ENV || 'not set');\""
  },
  "dependencies": {
    "@speakbetter/mobile": "*",
    "@speakbetter/core": "*",
    "@speakbetter/api": "*",
    "@speakbetter/ui": "*",
    "@speakbetter/state": "*",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "react-native-config": "^1.5.1",
    "react-native-safe-area-context": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/metro-config": "^0.73.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "babel-jest": "^29.6.3",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "typescript": "5.0.4"
  }
}
